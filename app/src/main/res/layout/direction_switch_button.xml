<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="270dp"
    android:layout_height="270dp">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        >

        <ImageView
            android:id="@+id/ivLeftFour1111"
            android:layout_width="30dp"
            android:layout_height="26dp"
            android:layout_gravity="center_horizontal|top"
            android:layout_marginTop="40dp"
            android:scaleType="fitXY"
            android:src="@drawable/ic_up_default" />

        <ImageView
            android:id="@+id/ivLeftFour1122"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_gravity="center_horizontal|bottom"
            android:layout_marginBottom="40dp"
            android:scaleType="fitXY"
            android:src="@drawable/ic_down_default"/>

        <ImageView
            android:id="@+id/ivLeftFour1133"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_gravity="center_vertical|left"
            android:layout_marginLeft="40dp"
            android:scaleType="fitXY"
            android:src="@drawable/ic_left_default" />

        <ImageView
            android:id="@+id/ivLeftFour1144"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_gravity="center_vertical|right"
            android:layout_marginRight="40dp"
            android:scaleType="fitXY"
            android:src="@drawable/ic_right_default"/>

        <TextView
            android:id="@+id/tvLeftFour1111"
            android:layout_width="50dp"
            android:layout_gravity="center_horizontal|top"
            android:gravity="center"
            android:layout_marginTop="2dp"
            android:layout_height="34dp"
            android:scaleType="fitXY"
            android:text=""
            android:textSize="14dp"
            android:textColor="@color/white"/>

        <TextView
            android:id="@+id/tvLeftFour1122"
            android:layout_width="50dp"
            android:layout_gravity="center_horizontal|bottom"
            android:gravity="center"
            android:layout_marginBottom="2dp"
            android:layout_height="34dp"
            android:scaleType="fitXY"
            android:text=""
            android:textSize="14dp"
            android:textColor="@color/white"/>

        <TextView
            android:id="@+id/tvLeftFour1133"
            android:layout_width="wrap_content"
            android:layout_height="34dp"
            android:layout_gravity="center_vertical|left"
            android:layout_marginLeft="2dp"
            android:gravity="center"
            android:scaleType="fitXY"
            android:text=""
            android:textColor="@color/white"
            android:textSize="14dp" />

        <TextView
            android:id="@+id/tvLeftFour1144"
            android:layout_width="wrap_content"
            android:layout_gravity="center_vertical|right"
            android:gravity="center"
            android:layout_marginRight="2dp"
            android:layout_height="34dp"
            android:scaleType="fitXY"
            android:text=""
            android:textSize="14dp"
            android:textColor="@color/white"/>

        <FrameLayout
            android:layout_width="110dp"
            android:layout_height="110dp"
            android:layout_gravity="center"
            android:background="@drawable/shape_ring_bg"
            >

            <TextView
                android:id="@+id/tvLeftXz"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_gravity="center"
                android:gravity="center"
                android:text=""
                android:textSize="18dp"
                android:textColor="@color/app_title_middle_txt"
                android:background="@drawable/shape_ring_bg"/>

            <ImageView
                android:id="@+id/ivLeftFourB1111"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:scaleType="fitXY"
                android:src="@drawable/shape_ring_bg_normal"
                android:layout_gravity="center_horizontal|top"
                android:layout_marginTop="3dp"
               />

            <ImageView
                android:id="@+id/ivLeftFourB1122"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_gravity="center_horizontal|bottom"
                android:layout_marginBottom="3dp"
                android:scaleType="fitXY"
                android:src="@drawable/shape_ring_bg_normal"/>

            <ImageView
                android:id="@+id/b81_iv"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_gravity="center_vertical|left"
                android:layout_marginLeft="3dp"
                android:scaleType="fitXY"
                android:src="@drawable/shape_ring_bg_normal" />

            <ImageView
                android:id="@+id/b82_iv"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_gravity="center_vertical|right"
                android:layout_marginRight="3dp"
                android:scaleType="fitXY"
                android:src="@drawable/shape_ring_bg_normal"/>

        </FrameLayout>




    </FrameLayout>
</FrameLayout>